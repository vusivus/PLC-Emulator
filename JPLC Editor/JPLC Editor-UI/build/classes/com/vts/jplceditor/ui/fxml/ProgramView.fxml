<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.vts.jplceditor.ui.controller.ProgramViewController">
   <children>
      <Label layoutX="7.0" layoutY="81.0" text="Program Name" />
      <Label layoutX="14.0" layoutY="25.0" text="Create Ladder Program">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Button fx:id="acceptButton" layoutX="90.0" layoutY="141.0" mnemonicParsing="false" onAction="#acceptButtonAction" text="Accept">
         <graphic>
            <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../icon/ok-apply-icon.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="cancelButton" layoutX="188.0" layoutY="141.0" mnemonicParsing="false" onAction="#cancelButtonAction" text="Cancel">
         <graphic>
            <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../icon/cancel-icon.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <TextField fx:id="nameTF" layoutX="97.0" layoutY="77.0" />
   </children>
</AnchorPane>
